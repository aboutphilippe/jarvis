{
  "name": "jarvis-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start.watch": "nodemon src/server.ts",
    "dev": "pnpm start.watch",
    "ngrok": "ngrok http 8080",
    "type-check": "tsc --noEmit"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "tsx"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "ai": "latest",
    "dotenv": "16.4.5",
    "express": "5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/node": "20.11.20",
    "tsx": "4.7.1",
    "typescript": "5.5.4",
    "nodemon": "^2.0.22"
  }
}
